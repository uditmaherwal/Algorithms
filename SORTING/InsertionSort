def insertionsort(array):
    for loop_index in range(1, len(array)):
        insertion_index = loop_index
        while (
            insertion_index > 0
            and array[insertion_index - 1] > arrayn_index]
        ):
            array[insertion_index], array[insertion_index - 1] = (
                array[insertion_index - 1],
                array[insertion_index],
            )
            insertion_index -= 1

    return array
